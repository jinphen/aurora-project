                <ss:attribute name="endOnTick" default="false" document="坐标轴是否以刻度线为末端" type="ss:boolean"/>
                <ss:attribute name="gridLineColor" default="#C0C0C0" document="网格线的颜色" type="ss:String"/>
                <ss:attribute name="gridLineDashStyle" default="Solid" document="网格线的样式" type="a:dashStyleType"/>
                <ss:attribute name="gridLineWidth" default="0" document="网格线的宽度" type="ss:int"/>
                <ss:attribute name="id" document="可通过脚本chart.get(id)获得坐标轴对象" type="ss:String"/>
                <ss:attribute name="lineColor" default="#C0D0E0" document="坐标轴的颜色" type="ss:String"/>
                <ss:attribute name="lineWidth" default="1" document="坐标轴的宽度" type="ss:int"/>
                <ss:attribute name="linkedTo" document="坐标系关联" type="ss:int"/>
                <ss:attribute name="max" document="坐标轴显示的最大值" type="ss:int"/>
                <ss:attribute name="maxPadding" default="0.01" document="坐标轴显示的最大值离数据图形峰值的间距，如该值为0.05,则间距为峰值高度乘以0.05。当max属性被设置时，该属性无效。" type="ss:int"/>
                <!--maxZoom deprecated-->
                <ss:attribute name="min" document="坐标轴显示的最小值" type="ss:int"/>
                <ss:attribute name="minPadding" default="0.01" document="坐标轴显示的最小值离数据图形谷值的间距，如该值为0.05,则间距为谷值高度乘以0.05。当min属性被设置时，该属性无效。" type="ss:int"/>
                <ss:attribute name="minRange" document="坐标系缩放时的最小范围值" type="ss:int"/>
                <ss:attribute name="minTickInterval" document="最小刻度间隔值" type="ss:int"/>
                <ss:attribute name="minorGridLineColor" default="#E0E0E0" document="次级网格线的颜色" type="ss:String"/>
                <ss:attribute name="minorGridLineDashStyle" default="Solid" document="次级网格线的样式" type="a:dashStyleType"/>
                <ss:attribute name="minorGridLineWidth" default="1" document="次级网格线的宽度" type="ss:int"/>
                <ss:attribute name="minorTickColor" default="#A0A0A0" document="次级刻度线的颜色" type="ss:String"/>
                <ss:attribute name="minorTickInterval" document="次级刻度线的间隔值" type="ss:int"/>
                <ss:attribute name="minorTickLength" default="2" document="次级刻度线的长度" type="ss:int"/>
                <ss:attribute name="minorTickPosition" default="outside" document="次级刻度线的位置" type="a:tickPositionType"/>
                <ss:attribute name="minorTickWidth" default="0" document="次级刻度线的宽度" type="ss:int"/>
                <ss:attribute name="name" document="组件name,对应dataset中field的name" type="ss:String"/>
                <ss:attribute name="offset" default="0" document="坐标轴到图形区域的距离" type="ss:int"/>
                <ss:attribute name="opposite" default="false" document="坐标轴是否在图形区域的另一边" type="ss:boolean"/>
                <ss:attribute name="reversed" default="false" document="坐标轴反向" type="ss:boolean"/>
                <ss:attribute name="showEmpty" default="true" document="当该轴对应的数据通过点击legend隐藏后，是否显示坐标轴" type="ss:boolean"/>
                <ss:attribute name="showFirstLabel" default="true" document="显示坐标轴第一个刻度上的标识" type="ss:boolean"/>
                <ss:attribute name="showLastLabel" default="false" document="显示坐标轴最后一个刻度上的标识" type="ss:boolean"/>
                <ss:attribute name="startOfWeek" default="1" document="设定日期型坐标轴的刻度线上显示的日期。0 = Sunday, 1 = Monday." type="ss:int"/>
                <ss:attribute name="startOnTick" default="false" document="是否以刻度线为起点" type="ss:boolean"/>
                <ss:attribute name="tickColor" default="#C0D0E0" document="刻度线的颜色" type="ss:String"/>
                <ss:attribute name="tickInterval" document="刻度线的间隔值" type="ss:int"/>
                <ss:attribute name="tickLength" default="5" document="刻度线的长度" type="ss:int"/>
                <ss:attribute name="tickPixelInterval" document="如果tickInterval是null，则此选项设置近似的像素间隔的刻度线" type="ss:int"/>
                <ss:attribute name="tickPosition" default="outside" document="刻度线的位置" type="a:tickPositionType"/>
                <ss:attribute name="tickPositier" document="刻度线的定位函数function(){return []},返回数组" type="ss:String"/>
                <ss:attribute name="tickPositions" document="刻度线的定位，如0,1,2,4,8" type="ss:String"/>
                <ss:attribute name="tickWidth" default="1" document="刻度线的宽度" type="ss:int"/>
                <ss:attribute name="tickmarkPlacement" default="between" document="坐标轴上的label是在刻度线上还是在刻度线与刻度线之间" type="a:placementType"/>
                <ss:attribute name="type" default="linear" document="坐标轴类型" type="a:axisType"/>
            </ss:attributes>
            <ss:elements>
                <ss:element ref="a:labels"/>
                <ss:element ref="a:title"/>
                <ss:element ref="a:dateTimeLabelFormats"/>
            </ss:elements>
            <ss:arrays>
                <ss:array name="a:events" document="事件" indexField="name" type="a:event"/>
                <ss:array name="a:plotBands" document="网格区域配置" type="a:plotBand"/>
                <ss:array name="a:plotLines" document="网格线配置" type="a:plotLine"/>
            </ss:arrays>
        </ss:element>
        <ss:element name="a:yAxi">
            <ss:attributes>
                <ss:attribute name="bindTarget" document="存放动态字段的Dataset" type="a:DataSetReference"/>
                <ss:attribute name="endOnTick" default="true" document="坐标轴是否以刻度线为末端" type="ss:boolean"/>
                <ss:attribute name="gridLineWidth" default="1" document="网格线的宽度" type="ss:int"/>
                <ss:attribute name="lineWidth" default="0" document="坐标轴的宽度" type="ss:int"/>
                <ss:attribute name="maxPadding" default="0.05" document="坐标轴显示的最大值离数据图形峰值的间距，如该值为0.05,则间距为峰值高度乘以0.05。当max属性被设置时，该属性无效。" type="ss:int"/>
                <ss:attribute name="minPadding" default="0.05" document="坐标轴显示的最小值离数据图形谷值的间距，如该值为0.05,则间距为谷值高度乘以0.05。当min属性被设置时，该属性无效。" type="ss:int"/>
                <ss:attribute name="showLastLabel" default="true" document="显示坐标轴最后一个刻度上的标识" type="ss:boolean"/>
                <ss:attribute name="startOnTick" default="true" document="是否以刻度线为起点" type="ss:boolean"/>
            </ss:attributes>
            <ss:elements>
                <ss:element ref="a:stackLabels"/>
            </ss:elements>
            <ss:extensions>
                <ss:extension base="a:xAxi"/>
            </ss:extensions>
        </ss:element>
        <ss:element name="a:dateTimeLabelFormats" document="日期型label的格式化">
            <ss:attributes>
                <ss:attribute name="millisecond" default="%A, %b %e, %H:%M:%S.%L" document="毫秒" type="ss:String"/>
                <ss:attribute name="second" default="%A, %b %e, %H:%M:%S" document="秒" type="ss:String"/>
                <ss:attribute name="minute" default="%A, %b %e, %H:%M" document="分" type="ss:String"/>
                <ss:attribute name="hour" default="%A, %b %e, %H:%M" document="时" type="ss:String"/>
                <ss:attribute name="day" default="%A, %b %e, %Y" document="日" type="ss:String"/>
                <ss:attribute name="week" default="Week from %A, %b %e, %Y" document="周" type="ss:String"/>
                <ss:attribute name="month" default="%B %Y" document="月" type="ss:String"/>
                <ss:attribute name="year" default="%Y" document="年" type="ss:String"/>
            </ss:attributes>
        </ss:element>
        <ss:element name="a:plotBand">
            <ss:attributes>
                <ss:attribute name="color" document="网格块颜色" type="ss:String"/>
                <ss:attribute name="from" document="网格块的开始值" type="ss:String"/>
                <ss:attribute name="id" document="网格块的id，通过脚本Axis.removePlotBand(id)来移除" type="ss:int"/>
                <ss:attribute name="innerRadius" document="内半径" type="ss:String"/>
                <ss:attribute name="outerRadius" document="外半径" type="ss:String"/>
                <ss:attribute name="thickness" document="厚度" type="ss:String"/>
                <ss:attribute name="to" document="网格块的结束值" type="ss:String"/>
                <ss:attribute name="zIndex" document="网格块的层的index" type="ss:int"/>
            </ss:attributes>
            <ss:elements>
                <ss:element ref="a:plotLabel"/>
            </ss:elements>
            <ss:arrays>
                <ss:array name="a:events" document="事件" indexField="name" type="a:event"/>
            </ss:arrays>
        </ss:element>
        <ss:element name="a:plotLine">
            <ss:attributes>
                <ss:attribute name="color" document="网格线颜色" type="ss:String"/>
                <ss:attribute name="dashStyle" document="网格线的样式" type="a:dashStyleType"/>
                <ss:attribute name="id" document="网格线的id，通过脚本Axis.removePlotLine(id)来移除" type="ss:int"/>
                <ss:attribute name="value" document="网格线所在的值" type="ss:String"/>
                <ss:attribute name="width" document="网格线的宽度" type="ss:String"/>
                <ss:attribute name="zIndex" document="网格线的层的index" type="ss:int"/>
            </ss:attributes>
            <ss:elements>
                <ss:element ref="a:plotLabel"/>
            </ss:elements>
            <ss:arrays>
                <ss:array name="a:events" document="事件" indexField="name" type="a:event"/>
            </ss:arrays>
        </ss:element>
        <ss:element name="a:plotLabel">
            <ss:attributes>
                <ss:attribute name="rotation" document="旋转度数" type="ss:int"/>
                <ss:attribute name="textAlign" document="文本对齐方式" type="a:alignType"/>
                <ss:attribute name="useHTML" default="false" document="使用HTML元素生成label" type="ss:boolean"/>
            </ss:attributes>
            <ss:extensions>
                <ss:extension base="a:legendTitle"/>
                <ss:extension base="a:position"/>
            </ss:extensions>
        </ss:element>
        <ss:element name="a:tooltip" document="跟踪浮动窗口">
            <ss:attributes>
                <ss:attribute name="animation" default="true" document="是否动画效果" type="ss:boolean"/>
                <ss:attribute name="backgroundColor" default="rgb(255,255,255)" document="背景颜色" type="ss:String"/>
                <ss:attribute name="borderColor" default="auto" document="边框颜色" type="ss:String"/>
                <ss:attribute name="borderRadius" default="3" document="边框半径" type="ss:int"/>
                <ss:attribute name="borderWidth" default="1" document="边框宽度" type="ss:int"/>
                <ss:attribute name="crosshairs" document="是否有十字线" type="ss:String"/>
                <ss:attribute name="enabled" default="true" document="是否启用" type="ss:boolean"/>
                <ss:attribute name="formatter" document="格式function(){return &apos;&apos;},请在函数中查看this对象的属性" type="ss:String"/>
                <ss:attribute name="positioner" document="tooltip定位函数function(){return {x:0,y:0}}" type="ss:String"/>
                <ss:attribute name="shadow" default="true" document="是否有阴影" type="ss:boolean"/>
                <ss:attribute name="shared" default="false" document="是否让多有series共享tooltip" type="ss:boolean"/>
                <ss:attribute name="snap" document="感应距离" type="ss:int"/>
                <ss:attribute name="style" document="样式" type="ss:String"/>
                <ss:attribute name="useHTML" default="false" document="使用HTML元素生成label" type="ss:boolean"/>
            </ss:attributes>
            <ss:extensions>
                <ss:extension base="a:plotTooltip"/>
            </ss:extensions>
            <ss:arrays>
                <ss:array name="a:crosshairs" document="十字线" type="a:crosshair"/>
            </ss:arrays>
        </ss:element>
        <ss:element name="a:crosshair" document="十字线">
            <ss:attributes>
                <ss:attribute name="color" document="颜色" type="ss:String"/>
                <ss:attribute name="dashStyle" default="Solid" document="线的样式" type="a:dashStyleType"/>
                <ss:attribute name="width" default="1" document="宽度" type="ss:int"/>
            </ss:attributes>
        </ss:element>
        <ss:element name="a:labels">
            <ss:attributes>
                <ss:attribute name="distance" document="距离(适用于gauge)" type="ss:int"/>
                <ss:attribute name="enabled" default="true" document="是否启用" type="ss:boolean"/>
                <ss:attribute name="format" default="{value}" document="格式化字符串" type="ss:String"/>
                <ss:attribute name="formatter" document="格式化函数function(){return &apos;&apos;},请在函数中查看this对象的属性" type="ss:String"/>
                <ss:attribute name="maxStaggerLines" default="5" document="最大错开行数" type="ss:int"/>
                <ss:attribute name="overflow" document="设为justify时，超出可视范围的标签会被挤在可视范围内" type="ss:String"/>
                <ss:attribute name="rotation" default="0" document="旋转度数" type="ss:int"/>
                <ss:attribute name="staggerLines" document="错开行数" type="ss:int"/>
                <ss:attribute name="step" document="步距" type="ss:int"/>
                <ss:attribute name="style" document="样式" type="ss:String"/>
                <ss:attribute name="useHTML" default="false" document="使用HTML元素生成label" type="ss:boolean"/>
                <ss:attribute name="zIndex" default="7" document="层的序列" type="ss:int"/>
            </ss:attributes>
            <ss:extensions>
                <ss:extension base="a:PositionAttrbute"/>
            </ss:extensions>
        </ss:element>
        <ss:element name="a:stackLabels">
            <ss:attributes>
                <ss:attribute name="enabled" default="true" document="是否启用" type="ss:boolean"/>
                <ss:attribute name="format" default="{value}" document="格式化字符串" type="ss:String"/>
                <ss:attribute name="formatter" document="格式化函数function(){return &apos;&apos;},请在函数中查看this对象的属性" type="ss:String"/>
            </ss:attributes>
            <ss:extensions>
                <ss:extension base="a:plotLabel"/>
            </ss:extensions>
        </ss:element>
        <ss:element name="a:fillColor">
            <ss:attributes>
                <ss:attribute name="linearGradient" document="渐进的方向，如:0,0,0,100" type="ss:String"/>
                <ss:attribute name="stops" document="渐进的颜色变化，如：[0,&apos;#fff&apos;],[1,&apos;#ccc&apos;]" type="ss:String"/>
            </ss:attributes>
        </ss:element>
        <ss:element name="a:backgroundColor">
            <ss:extensions>
                <ss:extension base="a:fillColor"/>
            </ss:extensions>
        </ss:element>
        <ss:element name="a:negativeFillColor">
            <ss:extensions>
                <ss:extension base="a:fillColor"/>
            </ss:extensions>
        </ss:element>
