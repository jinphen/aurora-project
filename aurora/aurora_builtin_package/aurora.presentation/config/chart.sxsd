            <ss:elements>
                <ss:element ref="a:marker"/>
                <ss:element ref="a:dataLabels"/>
            </ss:elements>
            <ss:arrays>
                <ss:array name="a:events" document="事件" indexField="name" type="a:event"/>
            </ss:arrays>
        </ss:element>
        <ss:element name="a:subtitle" document="副标题">
            <ss:attributes>
                <ss:attribute name="floating" document="是否浮动" type="ss:boolean"/>
                <ss:attribute name="useHTML" default="false" document="使用HTML元素生成label" type="ss:boolean"/>
            </ss:attributes>
            <ss:extensions>
                <ss:extension base="a:legendTitle"/>
                <ss:extension base="a:position"/>
            </ss:extensions>
