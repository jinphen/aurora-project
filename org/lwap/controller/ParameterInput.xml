<?xml version="1.0" ?>
<p:procedure xmlns:p="uncertain.proc">
	<p:action Name="BeginService" />
	<p:action Name="ParseParameter" />
	<p:action Name="PrepareService" />
	<p:action Name="CheckPrivilege" />
	<p:action Name="DoAction" />	
	<p:action Name="CreateView" />
	<exception-handles>
		<p:catch Exception="java.sql.SQLException" NextStep="CreateView">
			<p:set  Field="@ImportSuccess" Value="false" />
		</p:catch>
	</exception-handles>	
</p:procedure>